  <body>
    <!-- 회원가입 팝업 -->
    <div>

      <div class="ui-block-title bg-primary">
          <h6 class="align-center">
              <img src="/static/images/logo_w.png">
          </h6>
      </div>

      <form class="" action="/addRegister" method="post" enctype="multipart/form-data" name="reg">
      <div class="ui-block-content">
          <div class="form-group label-floating is-empty">
            <input type="hidden" name="username" id="username">
            <div id="image_preview" class="author-thumb profile">
              <img src="/static/images/logo_w.png" width="200px" height="200px" alt="" class="avatar">
              <input type="file" name="userImage" id="userImage">
            </div>



          </div>

          <div class="form-group label-floating is-empty">
              <label class="control-label">닉네임</label>
              <input type="text" name="displayName" value="" id="displayNameVal" placeholder="닉네임">
              <span id="resultDisplayName" class="material-input"></span>
          </div>

          <div class="form-group label-floating is-empty">
              <label >성별</label>
              <div class="">
                남<input type="radio" name="gender" value="m">
                여<input type="radio" name="gender" value="f">
                <span class="material-input"></span>
              </div>

          </div>

          <div class="form-group label-floating is-empty">
              <label class="control-label">생년월일</label>
              <input type="date" name="birthDay" value="">
              <span class="material-input"></span>
          </div>

          <div class="row">
              <div class="col-lg-12 col-sm-12">
                  <input type="submit" name="" class="btn btn-smoke btn-md btn-light-bg full-width" value="완료" id="save" onclick="parentSubmit();">
              </div>
          </div>
        </form>


          <hr/>


      </div>


    </div>




